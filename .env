# Do not edit this file directly! You can override one or more of these
# variables by defining them directly in the shell.

# Debug mode
DEBUG=False

# My Workshop
MYAPP_NAME=myworkshop
MYAPP_VERSION=0.1.0
MYAPP_INSTANCE_NAME=myworkshop
MYAPP_DOMAIN=example.com
MYAPP_SITE_NAME=My Workshop

# reverse-proxy
NGINX_VERSION=1.21
NGINX_CLIENT_MAX_BODY_SIZE=10M

# db
POSTGRES_VERSION=13
POSTGRES_DB=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# brocker
RABBITMQ_VERSION=3.9
RABBITMQ_DEFAULT_USER=guest
RABBITMQ_DEFAULT_PASS=guest
RABBITMQ_DEFAULT_VHOST=my_vhost

# core
PYTHON_VERSION=3.9
DJANGO_SECRET_KEY=secret
DJANGO_SUPERUSER_USERNAME=admin
DJANGO_SUPERUSER_PASSWORD=admin
DJANGO_SUPERUSER_EMAIL=admin@example.com
GUNICORN_WORKERS=
GUNICORN_LOG_LEVEL=INFO

# email
EMAIL_HOST=smtp.example.com
EMAIL_PORT=587
EMAIL_HOST_USER=webmaster@example.com
EMAIL_HOST_PASSWORD=password
EMAIL_USE_TLS=True
EMAIL_USE_SSL=False

# frontend
NODE_VERSION=16.11

# exposed ports
NGINX_HOST_PORT=8080
RABBITMQ_MANAGEMENT_HOST_PORT=15672

# exposed ports in development mode
POSTGRES_HOST_PORT_DEV=5432
RABBITMQ_HOST_PORT_DEV=5672
CORE_HOST_PORT_DEV=8000
FRONTEND_HOST_PORT_DEV=3000
